{
	"appName": "BCSSteam",
	"objName": "sfm",
	"serviceName": "getSfmByFilter",
	"tableName": "BCSSteam_sfm",
	"url": "sfm/getbyfilter",
	"query":
	{
		"cols": [
			"sfm_code",
			"sfm_description",
			"vendor_id",
			"model_no",
			"serial_no",
			"year_of_manufacturing",
			"working_principle_id",
			"size_id",
			"flow_rate_min",
			"flow_rate_max",
			"input_power_id",
			"output_signal_id",
			"connected_tnpcb",
			"tnpcb_ref",
			"connected_cpcb",
			"cpcb_ref",
			"remarks",
			"created_by",
			"created_on",
			"modified_by",
			"modified_on"
		],
		"filters": [
			{
				"col": "created_on",
				"op": ">=",
				"condition": "params.start_date",
				"value": "parseInt(params.start_date)",
				"paramName": "start_date"
			},
			{
				"col": "created_on",
				"op": "<=",
				"condition": "params.end_date",
				"value": "parseInt(params.end_date)",
				"paramName": "end_date"
			}
		],
		"limit": null,
		"groupBy": [],
		"orderBy": [],
		"sort": "asc",
		"offset": 0
	}
}