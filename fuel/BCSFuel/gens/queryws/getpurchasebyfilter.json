{
	"appName": "BCSFuel",
	"objName": "purchase",
	"serviceName": "getPurchaseByFilter",
	"tableName": "bcsfuel_purchase",
	"url": "purchase/getbyfilter",
	"query":
	{
		"cols": [
			"purchase_date",
			"fuel_code_id",
			"fuel_unit_id",
			"fuel_vendor_id",
			"fuel_qty",
			"fuel_value",
			"fuel_gst_value",
			"total_fuel_value",
			"unit_price",
			"remarks",
			"created_by",
			"created_on",
			"modified_by",
			"modified_on"
		],
		"filters": [
		{
			"col": "purchase_date",
			"op": ">=",
			"condition": "params.start_date",
			"value": "parseInt(params.start_date)",
			"paramName": "start_date"
		},
		{
			"col": "purchase_date",
			"op": "<=",
			"condition": "params.end_date",
			"value": "parseInt(params.end_date)",
			"paramName": "end_date"
		},
		{
			"col": "fuel_code_id",
			"op": "=",
			"condition": "params.fuel_code_id && params.fuel_code_id != 'all'",
			"value": "params.fuel_code_id"
		}],
		"limit": null,
		"groupBy": [],
		"orderBy": [],
		"sort": "asc",
		"offset": 0
	}
}